#!perl -l
use strict;
use App::perlbrew;
use Getopt::Long qw(:config pass_through);

my $opt = {
    force => undef,
    quiet => 1,
    D     => [],
};
my $result = GetOptions(
    'f|force!'  => \$opt->{force},
    'q|quiet!'  => \$opt->{quiet},
    'v|verbose' => \$opt->{verbose},
    'as=s'      => \$opt->{as},
    'D|D=s@'    => $opt->{D},
);
App::perlbrew->run_command( $opt, @ARGV );
